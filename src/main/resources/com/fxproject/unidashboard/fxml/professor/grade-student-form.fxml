<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fxproject.unidashboard.controller.GradeStudentController">
    <children>
        <ComboBox fx:id="comboGroups" layoutX="225.0" layoutY="114.0" prefWidth="150.0" />
        <ComboBox fx:id="comboStudents" layoutX="225.0" layoutY="197.0" prefWidth="150.0" />
        <ComboBox fx:id="comboSubjects" layoutX="225.0" layoutY="155.0" prefWidth="150.0" />
      <Button fx:id="cancelButton" layoutX="225.0" layoutY="327.0" mnemonicParsing="false" onAction="#cancelGrade" text="Cancel" />
      <Button fx:id="submitButton" layoutX="321.0" layoutY="327.0" mnemonicParsing="false" onAction="#submitGrade" text="Submit" />
      <Spinner fx:id="markSpinner" editable="true" layoutX="226.0" layoutY="229.0" />
      <TextField fx:id="description" layoutX="226.0" layoutY="263.0" promptText="Description" />
      <ComboBox fx:id="comboTypes" layoutX="225.0" layoutY="295.0" prefWidth="150.0" />
    </children>
</AnchorPane>
